pub fn add(left: u64, right: u64) -> u64 {
    left + right
}

#[cfg(test)]
mod tests {
    use super::*;

    #[test]
    fn it_works() {
        let result = add(2, 2);
        assert_eq!(result, 4);
    }
}


/* run test it_works

a) add to Cargo,toml

[[test]]
name = "it_works"
path = "tests/test_2,rs"

b) run with command on command line

cargo test --test it_works
cargo test it_works


*/
